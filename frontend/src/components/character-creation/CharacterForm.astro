---
import * as content from '../../content/pages/character-creation.md'
import StyledMarkdown from '../shared/StyledMarkdown.astro'
---

<div class='mx-auto max-w-screen-xl px-4 py-8 sm:px-6 sm:py-12 lg:px-8 lg:py-16'>
  <h2 class='font-large pb-3 pl-0 text-5xl'>{content.frontmatter.header}</h2>
  <StyledMarkdown of={content.frontmatter.body} />
</div>

<iframe
  width='750px'
  height='960px'
  src='https://forms.office.com/Pages/ResponsePage.aspx?id=Kcp03DNgs0WGTvjDr9qTEsZMM3QqaIFKupWHSQgXTfFUMDJNNTU1REJUWVJCMkZBVUlBTlBTMVFQSyQlQCN0PWcu&embed=true'
  frameborder='0'
  marginwidth='0'
  marginheight='0'
  class='mx-auto max-w-screen-xl px-4 py-8 sm:px-6 sm:py-12 lg:px-8 lg:py-16'
  style='border: none; max-width:100%; max-height:100vh'
  allowfullscreen
  webkitallowfullscreen
  mozallowfullscreen
  msallowfullscreen
>
</iframe>
